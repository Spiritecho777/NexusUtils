<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="250" d:DesignHeight="150"
        x:Class="NexusUtils.Popup.CredSitePopup"
        Title="CredSitePopup" Height="150" Width="250">
  <Grid>
    <TextBox x:Name="Identifiant" Watermark="Identifiant" Height="15" Width="230" Margin="5,10,0,0" VerticalAlignment="Top"/>
    <TextBox x:Name="Password" Watermark="Mot de passe" Height="15" Width="230" Margin="5,50,0,0" VerticalAlignment="Top"/>
    <Button Content="Ajouter" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="10" Click="Add_Click"/>
    <Button Content="Annuler" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="10" Click="Cancel_Click"/>
  </Grid>
</Window>
